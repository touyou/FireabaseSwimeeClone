# Uncomment this line to define a global platform for your project
platform :ios, '9.0'

target 'FirebaseSwimee' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FirebaseSwimee
  pod 'Firebase'
  pod 'Firebase/Database'

  target 'FirebaseSwimeeTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FirebaseSwimeeUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
